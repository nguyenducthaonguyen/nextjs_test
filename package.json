{
  "name": "techtus-nextjs",
  "version": "1.0.0",
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=22.16.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "i18n:import": "node .i18n/script.js --source .i18n/source.xlsx --sheet-names \"Locale Messages, UI_Message\" --our-dir lang --ignored-cols \"Screen ID, Note, Type, Description, Created by\"",
    "check-types": "tsc --noEmit --pretty",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "lighthouse": "unlighthouse-ci --site localhost:3000",
    "test:lighthouse": "run-p start lighthouse --race"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^9.15.0",
    "@t3-oss/env-nextjs": "^0.13.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "^15.4.2",
    "next-intl": "^4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@faker-js/faker": "^9.7.0",
    "@next/bundle-analyzer": "^15.3.1",
    "@next/eslint-plugin-next": "^15.3.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.12",
    "@types/react": "^19.1.3",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.1.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.89.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "unlighthouse": "^0.17.1",
    "vite": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.0",
    "vitest-fail-on-console": "^0.7.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  }
}
